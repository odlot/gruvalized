  "tokenColors": [
    {
      "name": "Comments",
      "scope": [
        "comment",
        "punctuation.definition.comment"
      ],
      "settings": {
        "fontStyle": "italic",
        "foreground": "#AAAAAA"
      }
    },
    {
      "name": "Doc Comments",
      "scope": [
        "comment.documentation",
        "comment.block.documentation"
      ],
      "settings": {
        "foreground": "#98971A"
      }
    },
    {
      "name": "Invalid - Illegal",
      "scope": "invalid.illegal",
      "settings": {
        "foreground": "#660000"
      }
    },
    {
      "name": "Operators",
      "scope": "keyword.operator",
      "settings": {
        "foreground": "#777777"
      }
    },
    {
      "name": "Keywords",
      "scope": [
        "keyword",
        "storage"
      ],
      "settings": {
        "foreground": "#458588"
      }
    },
    {
      "name": "Types",
      "scope": [
        "storage.type",
        "support.type"
      ],
      "settings": {
        "foreground": "#B16286"
      }
    },
    {
      "name": "Language Constants",
      "scope": [
        "constant.language",
        "support.constant",
        "variable.language"
      ],
      "settings": {
        "foreground": "#AB6526"
      }
    },
    {
      "name": "Variables",
      "scope": [
        "variable",
        "support.variable"
      ],
      "settings": {
        "foreground": "#B16286"
      }
    },
    {
      "name": "Functions",
      "scope": [
        "entity.name.function",
        "support.function"
      ],
      "settings": {
        "fontStyle": "bold",
        "foreground": "#AA3731"
      }
    },
    {
      "name": "Strings",
      "scope": "string",
      "settings": {
        "foreground": "#98971A"
      }
    },
    {
      "name": "Numbers",
      "scope": [
        "constant.numeric",
        "constant.character",
        "constant"
      ],
      "settings": {
        "foreground": "#AB6526"
      }
    }
  ],
  "semanticHighlighting": true,
  "semanticTokenColors": {
    "namespace": "#B16286",
    "class": {
      "bold": true,
      "foreground": "#B16286"
    },
    "interface": {
      "bold": true,
      "foreground": "#B16286"
    },
    "enum": {
      "foreground": "#AB6526"
    },
    "typeParameter": "#B16286",
    "parameter": "#B16286",
    "variable": "#B16286",
    "property": "#B16286",
    "function": {
      "foreground": "#AA3731",
      "bold": true
    },
    "method": {
      "foreground": "#AA3731",
      "bold": true
    },
    "string": "#98971A",
    "number": "#AB6526",
    "regexp": "#458588",
    "comment": "#AAAAAA",
    "keyword": "#458588",
    "operator": "#777777",
    "modifier": "#458588",
    "deprecated": {
      "strikethrough": true
    }
  }
}